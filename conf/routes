# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()
POST 	/contactus/submit			controllers.Application.submitContactForm()

GET     /test1                      controllers.Application.test1()
GET     /test2                      controllers.Application.test2()


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)

# Manager
GET		/menu						controllers.Manager.menu()
GET		/dashboard					controllers.Manager.dashboard()
GET		/login						controllers.Manager.login()
GET		/register					controllers.Manager.register()
